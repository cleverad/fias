{
    "name": "marvin255/fias",
    "description": "fias parser for php",
    "type": "library",
    "keywords": ["php", "fias"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "*",
        "sebastian/phpcpd": "*",
        "phpmetrics/phpmetrics": "*"
    },
    "autoload": {
        "psr-4": {
            "marvin255\\fias\\": "src/",
            "marvin255\\fias\\tests\\": "tests/src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/marvin255/fias"
        }
    ]
}
